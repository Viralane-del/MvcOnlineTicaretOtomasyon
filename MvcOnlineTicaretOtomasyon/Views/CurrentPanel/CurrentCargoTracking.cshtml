@using MvcOnlineTicaretOtomasyon.Models.Classes
@model List<CargoTracking>
@{
    ViewBag.Title = "CurrentCargoTracking";
    Layout = "~/Views/Shared/_CurrentLayout.cshtml";
}
<br />
@{ int sirano = 0;}
<table class="table table-striped">
    <tr>
        <th>İşlemler</th>  
        <th>Açıklama</th>
        <th>Tarih</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@(++sirano)</td>
            <td>@item.Description</td>
            <td>@item.DateTime</td>
        </tr>
    }
</table>

